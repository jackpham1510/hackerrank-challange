{
  "name": "Hackerrank Challenge",
  "version": "1.0",
  "author": "Pham Dung",
  "description": "Hackerrank challenge tracking",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "128": "icons/128.png"
  },
  "page_action": {
    "default_popup": "popup/index.html",
    "default_title": "Hackerrank Challenge",
    "default_icon": "icons/16.png"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "*://*.dung.codes/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["*://*.hackerrank.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "manifest_version": 2
}